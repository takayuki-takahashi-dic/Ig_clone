<div class="container">
  <div class="jumbotron">
    <div class="row">
      <% @users.each do |user| %>

        <div class="col-sm-6 col-md-4">
          <div class="panel">
            <%= link_to image_tag(user.image.url, size: '230x230'), user_path(user.id) %>
          </div>
        </div>
      <% end %>
  </div>
</div>
